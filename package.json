{
  "name": "car-ui",
  "version": "0.1.1",
  "description": "User interface for drift car",
  "main": "src/main.js",
  "scripts": {
    "wp-dev": "webpack --watch --display-error-details",
    "wp-build": "cross-env NODE_ENV=production webpack",
    "dev": "run-p wp-dev electron-dev",
    "electron-dev": "cross-env NODE_ENV=development electron .",
    "electron-test": "cross-env NODE_ENV=test electron .",
    "electron-build": "electron-builder --dir --linux --armv7l",
    "build": "run-s wp-build electron-build",
    "test": "jest",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [
    "car",
    "education",
    "fuel cells"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chokidar": "^1.7.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "electron": "^2.0.18",
    "electron-builder": "^21.2.0",
    "file-loader": "^5.0.2",
    "jest": "^25.1.0",
    "jest-esm-transformer": "^1.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "pigpio-mock": "0.0.1",
    "style-loader": "^1.1.3",
    "svelte": "^3.17.0",
    "svelte-loader": "^2.13.6",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "chart.js": "^2.9.3",
    "chartjs-plugin-zoom": "^0.7.5",
    "drivelist": "^5.0.0",
    "excel4node": "^1.7.2",
    "noble": "^1.9.1",
    "pigpio": "^3.0.0",
    "usb-detection": "^3.0.0"
  },
  "jest": {
    "transform": {
      ".mjs": "jest-esm-transformer"
    },
    "moduleFileExtensions": [
      "js",
      "mjs",
      "json"
    ]
  },
  "prettier": {
    "svelteSortOrder": "scripts-markup-styles",
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "build": {
    "linux": {
      "executableName": "CarController",
      "category": "education"
    }
  }
}
